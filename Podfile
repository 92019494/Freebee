# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Freebee' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Freebee
pod 'Firebase/Core'
pod 'Firebase/Database'
pod 'Firebase/Firestore'
pod 'Firebase/Auth'
pod 'Firebase/AdMob'
pod 'FBSDKLoginKit'
pod 'FBSDKShareKit'
pod 'FBSDKCoreKit'



  target 'FreebeeTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FreebeeUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
